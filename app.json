{
  "expo": {
    "name": "DEEDS",
    "slug": "DEEDS",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./app/assets/school-logo-with-photo.png",
      "resizeMode": "contain",
      "backgroundColor": "#041e42"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.camberwellgrammer.deedsstudent"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/icon.png",
        "backgroundColor": "#041e42"
      },
      "package": "com.camberwellgrammer.deedsstudent"
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    }
  }
}
